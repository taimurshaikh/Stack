{
    "manifest_version": 3,
    "version": "1.0.0",
    "name": "TabZen",
    "action": {
        "default_popup": "index.html"
    },
    "permissions": [
        "tabs",
        "tabGroups",
        "storage",
        "activeTab"
      ],
    "background": {
        "service_worker": "js/background.js",
        "type": "module"
    }
}